"Y"===b.storedAccountIsPreferreds[1]?$("#preferred-sa2").show():$("#preferred-sa2").hide();"Y"===b.storedAccountIsPreferreds[2]?$("#preferred-sa3").show():$("#preferred-sa3").hide();a.displayCardImage(a,b,0);a.displayCardImage(a,b,1);a.displayCardImage(a,b,2);$("#cardtype1").prop("checked",!1);$("#cardtype2").prop("checked",!1);$("#cardtype3").prop("checked",!1);$("#savecard").hide()},viewTwoCards:function(a,b){$("#sa1").show();$("#sa2").show();"Y"===b.storedAccountIsPreferreds[0]?$("#preferred-sa1").show():
$("#preferred-sa1").hide();"Y"===b.storedAccountIsPreferreds[1]?$("#preferred-sa2").show():$("#preferred-sa2").hide();a.displayCardImage(a,b,0);a.displayCardImage(a,b,1);$("#cardtype1").prop("checked",!1);$("#cardtype2").prop("checked",!1);$("#savecard").show()},viewUPTEorUPTM:function(a){"Accounts Receivable"===a&&($("#newcardpanel").show(),$("#useCreditCardWrap").show(),$("#please").hide(),$("#newcardpanel").hide(),$("#card-nickname-container").hide(),$("#paymentMethod").val("CC"))},viewUPTTorNCMSorMMWorDefault:function(a,
b){"Checkout"===b.pageTitle&&"Y"!==a.limited&&($("#newcardpanel").show(),$("#useCreditCardWrap").show(),$("#continue-link-container").hide(),$("#please").hide(),$("#newcardpanel").hide(),$("#card-nickname-container").hide(),$("#visaCheckoutContainer").removeClass("active-pills"),$("#diff-card").on("click touch",function(){$("#newcardpanel").show();$("#continue-link-container").show()}),$("#paymentMethod").val("CC"));"Y"===a.limited&&$("#continue-link-container").hide()}});
